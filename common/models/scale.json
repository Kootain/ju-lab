{
  "name": "Scale",
  "base": "PersistedModel",
  "strict": false,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": false,
      "comments": "称名称"
    },
    "pos": {
      "type": "string",
      "required": false,
      "comments": "称摆放位置坐标，'x,y'左下角为1,1"
    },
    "sid": {
      "type": "string",
      "required": false,
      "comments": "称SID"
    },
    "scalelog_id": {
      "type": "number"
    },
    "status": {
      "type": "number",
      "comments": "称是否在线"
    }
  },
  "validations": [],
  "relations": {
    "scalelogs": {
      "type": "belongsTo",
      "model": "Scalelog",
      "foreignKey": "scalelog_id",
      "scope": {
        "include": "items"
      }
    }
  },
  "acls": [],
  "methods": {}
}
